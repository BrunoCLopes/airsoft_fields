{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.14"
  },
  "scripts": {
    "tailwind:watch": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch",
    "django:server": "python manage.py runserver",
    "start": "concurrently \"npm run tailwind:watch\" \"npm run django:server\""
  }
}
